{
	"name": "uelements",
	"description": "uelements provides a functional way of defining custom elements",
	"version": "0.0.2",
	"source": "./src/index.ts",
	"main": "./dist/uelements.umd.js",
	"module": "./dist/uelements.es.js",
	"unpkg": "./dist/uelements.umd.js",
	"types": "index.d.ts",
	"files": [
		"dist/",
		"example/",
		"*.d.ts",
		"README.md"
	],
	"scripts": {
