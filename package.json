		"elements",
		"define",
		"lifecycle",
		"web components",
		"components",
		"custom element",
		"hooks",
		"react",
		"preact"
	],
	"author": "Kethan Surana",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kethan/uelements/issues"
	},
	"homepage": "https://github.com/kethan/uelements#readme",
