{
	"name": "uwhen-sinuous",
	"description": "HTML elements with sinuous and swiss",
	"version": "0.0.7",
	"source": "./src/index.ts",
	"main": "./dist/uwhen-sinuous.umd.js",
	"module": "./dist/uwhen-sinuous.es.js",
	"unpkg": "./dist/uwhen-sinuous.umd.js",
	"types": "index.d.ts",
	"files": [
		"dist/",
		"example/",
		"*.d.ts",
		"README.md"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview"
	},
	"exports": {
		".": {
			"import": "./dist/uwhen-sinuous.es.js",
			"require": "./dist/uwhen-sinuous.umd.js"
		}
	},
	"devDependencies": {
		"@types/node": "^16.0.0",
		"typescript": "^4.3.2",
		"vite": "^2.3.8"
	},
	"directories": {
		"example": "example"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kethan/uwhen-sinuous.git"
	},
	"keywords": [
		"functional",
		"reactive",
		"declarative",
		"dom",
		"custom",
		"elements",
		"define",
		"lifecycle",
		"mount",
		"unmount",
		"hooks",
		"react",
		"preact"
	],
	"author": "Kethan Surana",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kethan/uwhen-sinuous/issues"
	},
	"homepage": "https://github.com/kethan/uwhen-sinuous#readme",
	"dependencies": {
		"sinuous": "^0.27.12",
		"swiss": "^2.4.0"
	}
}
